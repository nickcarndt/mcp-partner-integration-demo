{
  "version": 2,
  "functions": {
    "api/sse.ts": {
      "runtime": "nodejs18.x",
      "maxDuration": 60
    },
    "api/**/*.ts": {
      "runtime": "nodejs18.x"
    }
  },
  "headers": [
    {
      "source": "/api/sse",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache, no-transform"
        },
        {
          "key": "X-Accel-Buffering",
          "value": "no"
        }
      ]
    }
  ]
}

